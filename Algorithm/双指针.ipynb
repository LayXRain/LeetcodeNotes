   "cell_type": "code",
   "source": [
    "#移动零,双指针\n",
    "def yidong(nums):\n",
    "    n=len(nums)\n",
    "    i = 0\n",
    "    for j in range(n):\n",
    "        if nums[j]!=0:\n",
    "            nums[i]=nums[j]\n",
    "            nums[j]=0\n",
    "            i+=1\n",
    "    return nums\n",
    "nums=[0,1,2,3,5,0,0,225,2452234,530,34,0,678,5]\n",
    "print(yidong(nums))"
   ],
